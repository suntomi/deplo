{name}:
  needs: [{needs}]
  if: ${{{{ needs.deplo-main.outputs.{name} && !failure() }}}}
  name: Running job {name}
  runs-on: {machine}
{job_envs:>2}
{container:>2}
  outputs:
    need-cleanup: ${{{{ steps.deplo-job-{name}.outputs.need-cleanup }}}}
    system: ${{{{ steps.deplo-job-{name}.outputs.system }}}}
    user: ${{{{ steps.deplo-job-{name}.outputs.user }}}}
  steps:
{fetchcli:>4}
{checkout:>4}
{caches:>4}
    - name: {name}
      id: deplo-job-{name}
{command:>6}
{debugger:>4}
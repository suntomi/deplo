{
    "version": "0.2.0",
    "configurations": [{
        "name": "Release",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/target/debug/cli",
        "args": [
            "-v=3", "vcs", "release", "nightly", "-o", "name=nightly"
        ],
        "preLaunchTask": "git reset --hard"
    },
    {
        "name": "Kick",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/target/debug/cli",
        "args": [
            "-v=3", "ci", "kick"
        ],
        "preLaunchTask": "git reset --hard"
    },
    {
        "name": "Deploy",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/target/debug/cli",
        "args": [
            "ci", "deploy", "product"
        ],
        "preLaunchTask": "git reset --hard"
    },
    {
        "name": "Init",
        "type": "lldb",
        "request": "launch",
        "program": "${workspaceFolder}/target/debug/cli",
        "args": [
            "init"
        ],
        "preLaunchTask": "git reset --hard"
    }]
}